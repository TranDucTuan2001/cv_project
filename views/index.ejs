                                    <div class="sec-title">
                                        <h2>Education Details</h2>
                                    </div>
                                    <div class="service no-margin row">
                                        <div class="col-sm-3 resume-dat serv-logo">
                                            <h6>
                                                <%= education.startDate %>-<%= education.endDate %>
                                            </h6>
                                            <p>Master Degree</p>
                                        </div>
                                        <div class="col-sm-9">
                                            <h5>
                                                <%= education.school %>
                                            </h5>
                                            <p>
